          },
          {
            "key": "time_interval",
            "name": "周期",
            "tip": "Bar的周期",
            "type": "select",
            "data": [
              {"value": "30s", "name": "30s"},
              {"value": "1m", "name": "1min"},
              {"value": "3m", "name": "3min"},
              {"value": "5m", "name": "5min"},
              {"value": "10m", "name": "10min"},
              {"value": "15m", "name": "15min"}
