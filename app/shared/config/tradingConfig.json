        "name": "BAR",
        "config": [
          {
            "key": "open",
            "name": "开启",
            "tip": "开启计算Bar数据进程",
            "type": "process",
            "target": "bar",
            "start": "startBar",
            "args": ["source", "time_interval"],
            "log_path": "bar"
          },
          {
            "key": "source",
