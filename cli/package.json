{
  "name": "kungfu-cli",
  "author": "taurus.ai",
  "version": "2.0.0",
  "description": "KungFu Trader Cli",
  "license": "Apache-2.0",
  "config": {
    "pyver": "three",
    "cmakejsopt": ""
  },
  "main": "./bin/cli.js",
  "engines": {
    "node": ">=8.0.0 <11.0.0"
  },
  "scripts": {
    "dev": "yarn run copy && node config/dev-cli.js",
    "webpack": "cross-env NODE_ENV=production node --max_old_space_size=8192 ../node_modules/webpack/bin/webpack.js --progress --colors --config config/webpack.cli.config.js",
    "build": "yarn run copy && yarn run webpack && yarn run pkg",
    "pkg": "pkg -t node10-macos-x64 . --out-path=build/",
    "copy": "sh ./scripts/copy.sh"
  },
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "4.2.0",
    "arch": "x64"
  },
  "bin": {
    "kungfu": "./bin/cli.js"
  },
  "pkg": {
    "assets": [
      "kfc/**/*",
      "resource/default/**"
    ]
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.16",
    "colors": "^1.3.3",
    "commander": "^2.20.0",
    "electron": "4.2.0",
    "fast-csv": "^2.5.0",
    "fkill": "^6.1.0",
    "fs-extra": "^7.0.1",
    "getprocesses": "^1.1.1",
    "inquirer": "^6.3.1",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "log4js": "^4.0.2",
    "moment": "^2.24.0",
    "pm2": "^3.5.0",
    "readline": "^1.3.0",
    "sqlite3": "^4.0.8",
    "tail": "^2.0.2",
    "taskkill": "^2.0.0"
  },
  "devDependencies": {
    "babel-loader": "^7.1.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "bindings": "^1.5.0",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "chalk": "^2.1.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.0.5",
    "cross-var": "^1.1.0",
    "css-loader": "^2.1.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "mkdirp": "^0.5.1",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "optimize-js-plugin": "^0.0.4",
    "pkg": "^4.4.0",
    "require-dir": "^0.3.0",
    "rimraf": "^2.6.3",
    "spectron": "^3.7.1",
    "webpack": "^3.5.2",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-dev-server": "3.0.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "webpack-permissions-plugin": "^1.0.0"
  }
}
